          "base_uri": "https://localhost:8080/",
          "height": 52
        },
        "outputId": "f5536076-2af7-44a6-9ef8-3f0da19235ab"
      },
      "source": [
        "str = \"Programming\"       # String\n",
        "print(type(str))\n",
        "print(len(str))"
      ],
      "execution_count": 5,
      "outputs": [
        {
          "output_type": "stream",
          "text": [
            "<class 'str'>\n",
            "11\n"
